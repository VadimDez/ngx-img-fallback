{
  "name": "ngx-img-fallback",
  "version": "2.0.0",
  "description": "Angular5+ directive for image fallback",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch false",
    "test:w": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "docs": "ng build --output-path docs --base-href ngx-img-fallback && cp ./docs/index.html ./docs/404.html",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://vadimdez@github.com/VadimDez/ngx-img-fallback.git"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular5",
    "angular5+",
    "img",
    "image",
    "fallback",
    "image-fallback"
  ],
  "author": "Vadym Yatsyuk <vadym.yatsyuk@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VadimDez/ngx-img-fallback/issues"
  },
  "homepage": "https://github.com/VadimDez/ngx-img-fallback#readme",
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular/animations": "^17.3.0",
    "@angular/cli": "^17.3.7",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@cypress/schematic": "^1.7.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.12.12",
    "cypress": "latest",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.3.0",
    "rxjs": "~7.8.0",
